{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/boards/zbefore/state/index.js",
    "pages/boards/zbefore/router_detail_read/index.js",
    "pages/boards/zbefore/router_detail_read/[number]/index.js",
    "pages/boards/zbefore/router_detail/index.js",
    "pages/boards/zbefore/new/index.js",
    "pages/boards/zbefore/mutation/index.js",
    "pages/_app.tsx"
  ],
  "exclude": ["node_modules"]
}
